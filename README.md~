sudo docker-compose run web django-admin startproject user_auth .
sudo chown -R $USER:$USER .
sudo docker-compose build
sudo docker-compose up
sudo docker-compose down
sudo docker exec django_registration_web_1 python manage.py startapp users
sudo docker exec django_registration_web_1 python manage.py makemigrationssudo sudo docker exec django_registration_web_1 python manage.py migrate
sudo docker exec -tiu postgres django_registration_db_1 psql
sudo docker exec -it django_registration_web_1 python manage.py shell

